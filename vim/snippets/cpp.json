{
  "Comment Block": {
    "prefix": "cc",
    "body": [
      "/**",
      " * $0",
      " */"
    ],
    "description": "(snippet) comment"
  },
  "cout << ... << endl": {
    "prefix": "cout",
    "body": [
      "std::cout << $0 << std::endl;"
    ],
    "description": "(snippet) cout + endl"
  },
  "Extern C Block": {
    "prefix": "cext",
    "body": [
      "#ifdef __cplusplus",
      "extern \"C\" {",
      "#endif",
      "",
      "$0",
      "",
      "#ifdef __cplusplus",
      "}",
      "#endif"
    ],
    "description": "(snippet) extern C"
  },
  "HYUNDAI MOTOR COMPANY Header": {
    "prefix": "hmc",
    "body": [
      "/**",
      " * ${TM_FILENAME} -",
      " *",
      " * Copyright (c) ${CURRENT_YEAR} HYUNDAI MOTOR COMPANY. ALL RIGHTS RESERVED.",
      " * Author: Insub Song (insub@hyundai.com)",
      " *",
      " */",
      "$0"
    ],
    "description": "(snippet) HYUNDAI MOTOR COMPANY"
  }
}
